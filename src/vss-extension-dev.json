{
    "manifestVersion": 1,
    "id": "dev-mightora-playwright-for-power-platform",
    "name": "DEV 1.1.67 - Playwright for Power Platform",
    "version": "1.1.67",
    "publisher": "mightoraio",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Automate end-to-end testing of Power Platform applications using Playwright in Azure DevOps pipelines with comprehensive reporting and failure analysis.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/mightora_io_icon_260x260.png"
    },
    "content": {
        "details": {
            "path": "readme.md"
        },
        "pricing": {
            "path": "pricing.md"
        }
    },
    "links": {
        "home": {
            "uri": "https://mightora.io"
        },
        "getstarted": {
            "uri": "https://mightora.io"
        },
        "learn": {
            "uri": "https://mightora.io"
        },
        "support": {
            "uri": "https://mightora.io"
        },
        "repository": {
            "uri": "https://mightora.io"
        },
        "issues": {
            "uri": "https://mightora.io"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/mightora"
    },
    "files": [
        {
            "path": "tasks/playwrightForPowerPlatform",
            "packagePath": "playwrightForPowerPlatform"
        },
        {
            "path": "tasks/playwrightForPowerPlatformAdvanced",
            "packagePath": "playwrightForPowerPlatformAdvanced"
        }
    ],
    "contributions": [
        {
            "id": "mightoria-playwrightForPowerPlatform",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "playwrightForPowerPlatform",
                "friendlyName": "Playwright for Power Platform"
             }
        },
        {
            "id": "mightoria-playwrightForPowerPlatformAdvanced",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "playwrightForPowerPlatformAdvanced",
                "friendlyName": "Playwright for Power Platform Advanced"
             }
        }
    ]
}